@model IEnumerable<you.Models.Data>

@{
    ViewBag.Title = "Beszállító alkalmazottak";
    Layout = "~/Views/Shared/_LayoutStocker.cshtml";
}

<h2>Beszállító alkalmazottak</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Supplier)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WorkerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IDCNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CarNPlate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BirthPlace)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BirthDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MothersName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedON)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Supplier)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WorkerName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IDCNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CarNPlate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BirthPlace)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BirthDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MothersName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedON)
        </td>
    </tr>
}

</table>
